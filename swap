using System;
					
public class Program
{
	public static void Main()
	 {
		int first_num;
		int second_num;
		int temp;

          Console.WriteLine("Please give a number as 1st variable: ");
	  first_num = Convert.ToInt32(Console.ReadLine());
	  Console.WriteLine("Please give a number as 2nd variable: ");
	  second_num = Convert.ToInt32(Console.ReadLine());
		
	  first_num = first_num ^ second_num;	
	  second_num = first_num ^ second_num;
	  first_num = first_num ^ second_num;
		
	
		
          Console.WriteLine("The value of the 1st variable: " + first_num);
	  Console.WriteLine("The value of the 1st variable: " + second_num);
    }
}
